model Jam {
    id          String @id @default(cuid())
    name        String
    description String
    bgImage     String

    authorId  String
    author    User     @relation(fields: [authorId], references: [id], onDelete: Cascade)
    createdAt DateTime @default(now())

    latitude  Float
    longitude Float
    accuracy  Float?

    @@map("jam")
}
